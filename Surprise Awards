import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int a,b;
		System.out.println("Enter the number");
		int n=sc.nextInt();
		a=n/10;
		b=n%10;
		if(((a==4 || a==7) && (b==4 || b==7)) || ((a+b)==4) || ((a+b)==7))
		System.out.println("YES");
		else if(n/100 ==7)
		System.out.println("YES");
		else
		System.out.println("NO");
	}
}
