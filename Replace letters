import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        String s=sc.next();
        char ch1=sc.next().charAt(0);
        char ch2=sc.next().charAt(0);
        char[] ch=s.toCharArray();
        for(int i=0;i<s.length();i++)
        {
            if(ch[i]==ch1)
            ch[i]=ch2;
            else if(ch[i]==ch2)
            ch[i]=ch1;
        }
        System.out.print(ch);
    }
}
