import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n,a,b;
		n=sc.nextInt();
		a=sc.nextInt();
		b=sc.nextInt();
		if(a==b)
		System.out.print(a+b);
		else if(a>b)
		System.out.print(a);
		else if(a==0)
	    System.out.print("5");
		else
		System.out.print(b);
	}
}
