import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n,x,y,z;
		n=sc.nextInt();
		x=sc.nextInt();
		y=sc.nextInt();
		z=(y*n)/100;
		if(x>=z && x!=1)
		System.out.print("0");
		else if(x==1)
		System.out.print("1");
		else
		System.out.print(z-x);
	}
}
