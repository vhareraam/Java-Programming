import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        double a = 0.5;
        System.out.print(a+" ");
        for(int i=1;i<n;i++)
        {
            a=a*3;
            if(i==(n-1))
            System.out.print(a);
            else
            System.out.print(a+" ");
        }
    }
}
