import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int d,s;
		float m;
		char b;
		d=sc.nextInt();
		s=sc.nextInt();
		m=sc.nextFloat();
		b=sc.next().charAt(0);
		if(b=='A')
		{
		    System.out.printf("%.2f%n",(s/m));
		    System.out.printf("0.00");
		}
		else if(b=='B')
		{
		    System.out.printf("%.2f%n",(d/m));
		    System.out.printf("%.2f",((s-d)/m));
		}
	}
}
