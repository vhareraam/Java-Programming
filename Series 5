import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n,a=4,i;
		n=sc.nextInt();
		for(i=0;i<n;i++)
		{
		    if(i==n-1)
		    {
		        System.out.print(a+(i*i));
		        a=a+(i*i);
		    }
		    else
		    {
		        System.out.print(a+(i*i) +" ");
		        a=a+(i*i);
		    }
		}
	}
}
